{
  "manifest": {
    "publishingInformation": {
      "locales": {
        "en-US": {
          "summary": "An Amazon Alexa skill useful for people living close to polar regions of the Earth.",
          "examplePhrases": [
            "Alexa, ask Polarcast how many days are left until we see the sun again.",
            "Alexa, ask Polarcast how long we are going to have daylight for.",
            "Alexa, ask Polarcast if I can see the Northern Lights in Longyearbyen today."
          ],
          "keywords": [
            "polar",
            "weather",
            "cold",
            "sun",
            "day",
            "cold"
          ],
          "name": "Polarcast",
